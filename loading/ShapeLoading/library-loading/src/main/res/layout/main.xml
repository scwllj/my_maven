<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:gpb="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <com.wllj.library.googleloading.GoogleProgressBar
        android:id="@+id/foldingCircles_progress"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_gravity="center"
        gpb:type="google_music_dices"/>
    <com.wllj.library.googleloading.GoogleProgressBar
        android:id="@+id/googleMusicDices_progress"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_gravity="center"
        gpb:type="folding_circles"/>
    <com.wllj.library.googleloading.GoogleProgressBar
        android:id="@+id/nexusRotationCross_progress"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_gravity="center"
        gpb:type="chrome_floating_circles"/>
    <com.wllj.library.googleloading.GoogleProgressBar
        android:id="@+id/chromeFloatingCircles_progress"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_gravity="center"
        gpb:type="nexus_rotation_cross"/>
</LinearLayout>
