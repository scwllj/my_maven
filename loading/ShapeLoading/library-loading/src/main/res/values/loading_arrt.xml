<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoadingView">

        <attr name="loadingText" format="string"/>
        <attr name="loadingTextAppearance" format="reference"/>
    </declare-styleable>

    <declare-styleable name="GoogleProgressBar">
        <attr name="type" format="enum">
            <enum name="folding_circles" value="0" />
            <enum name="google_music_dices" value="1" />
            <enum name="nexus_rotation_cross" value="2" />
            <enum name="chrome_floating_circles" value="3" />
        </attr>
        <attr name="colors" format="reference"/>
    </declare-styleable>
</resources>